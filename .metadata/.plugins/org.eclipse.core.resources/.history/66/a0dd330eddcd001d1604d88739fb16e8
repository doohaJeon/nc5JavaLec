package chap18_enum.enums;

public enum RGB {
	//상수명 RED("red", 255, 0, 0), GREEN("green", 0, 255, 0), BLUE("blue", 0,0,255)
	RED("red", 255, 0, 0), GREEN("grren", 0, 255, 0), BLUE("blue", 0, 0, 255);
	
	private String name;
	private int rgbVal1;
	private int rgbVal2;
	private int rgbVal3;
	
	RGB(String name, int rgbVal1, int rgbVal2, int rgbVal3) {
		this.name = name;
		this.rgbVal1 = rgbVal1;
		this.rgbVal2 = rgbVal2;
		this.rgbVal3 = rgbVal3;
	}
	
}
